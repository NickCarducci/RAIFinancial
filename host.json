{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information",
            "Host": "Error",
            "Function": "Error",
            "Host.Aggregator": "Error"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "samplingPercentage": 50
            }
        }
    },
    "extensionBundle": {
      "id": "Microsoft.Azure.Functions.ExtensionBundle",
      "version": "[4.*, 5.0.0)"
    },
    "concurrency": {
      "dynamicConcurrencyEnabled": true,
      "snapshotPersistenceEnabled": true
    }
}